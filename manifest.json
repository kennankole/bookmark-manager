{
  "manifest_version": 3,
  "name": "Study pilot",
  "description": "A Bookmark Manager that allows you to organize and structure your bookmarks",
  "version": "1.0",
  "permissions": ["bookmarks", "tabs", "history", "storage"],
  "background": {
    "service_worker": "src/modules/chromeTab.js",
    "type": "module"
  },

  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "default_popup": "/src/index.html",
    "default_title": "Let's study"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'"
  }
  
}