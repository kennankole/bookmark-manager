{
  "manifest_version": 3,
  "name": "Study pilot",
  "description": "A Bookmark Manager that allows you to organize and structure your bookmarks",
  "version": "1.0",
  "permissions": ["bookmarks"],
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Let's study"
  }
}